# ============================================
# RENTHUS - VARIÁVEIS DE AMBIENTE
# ============================================
# 
# IMPORTANTE: 
# 1. Copie este arquivo para .env
# 2. Preencha com suas chaves reais
# 3. NUNCA commite o arquivo .env no git
#
# ============================================

# ============================================
# SUPABASE - CONFIGURAÇÕES
# ============================================
# Obtenha estas chaves em: https://app.supabase.com/project/_/settings/api
#
# IMPORTANTE: Use apenas a URL e ANON KEY no app Flutter
# NUNCA use SERVICE_ROLE_KEY no cliente!

# Esta chave NÃO deve ser usada no app Flutter cliente!
# Ela bypassa todas as políticas RLS e é apenas para backend/edge functions
# SUPABASE_SERVICE_ROLE_KEY=sua_service_role_key_aqui

# ============================================
# FIREBASE - CONFIGURAÇÕES (OPCIONAL)
# ============================================
# Estas configurações já estão no firebase_options.dart
# Só descomente se precisar de configurações adicionais

# FIREBASE_API_KEY=sua_firebase_api_key
# FIREBASE_PROJECT_ID=seu_project_id
# FIREBASE_MESSAGING_SENDER_ID=seu_sender_id
# FIREBASE_APP_ID=seu_app_id

# ============================================
# AMBIENTE
# ============================================
# Valores: development, staging, production
ENVIRONMENT=development

# ============================================
# API KEYS DE TERCEIROS (se usar)
# ============================================
# GOOGLE_MAPS_API_KEY=sua_google_maps_key
# MERCADO_PAGO_PUBLIC_KEY=sua_mp_key
# STRIPE_PUBLISHABLE_KEY=sua_stripe_key

# ============================================
# CONFIGURAÇÕES DE DEBUG
# ============================================
# Habilitar logs detalhados (true/false)
DEBUG_MODE=true

# ============================================
# URLs DE API (se tiver backend adicional)
# ============================================
# API_BASE_URL=https://api.exemplo.com
# WEB_BASE_URL=https://app.exemplo.com
